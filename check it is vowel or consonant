#include<stdio.h>
int main()
{
    char ch;
    printf("enter a character\n");
    scanf("%c",&ch);
    if((ch >= 'a' && ch <= 'z') || (ch >='A' && ch <= 'Z'))
    {
        if((ch == 'a' || ch == 'e' || ch ==' i' || ch == 'o' || ch == 'u') || (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'))
    {
        printf("%c is vowel\n",ch);
    }else
     {
         printf("%c is consonant\n",ch);
    }
}else
{
    printf("invalid input\n");
}
return 0;
}
